import type ByteArray;
import type MarshalledBytes;

import throw func bytearray_new(size: uint) -> ByteArray;
import throw func bytearray_unmarshalBytes(mbytes: MarshalledBytes) -> ByteArray;
import throw func bytearray_size(ba: ByteArray) -> uint;
import throw func bytearray_getByte(ba: ByteArray, idx: uint) -> uint;
import throw func bytearray_setByte(ba: ByteArray, idx: uint, val: uint) -> ByteArray;
import throw func bytearray_get64(ba: ByteArray, idx: uint) -> uint;
import throw func bytearray_set64(ba: ByteArray, idx: uint, val: uint) -> ByteArray;
import throw func bytearray_get256(ba: ByteArray, offset: uint) -> uint;
import throw func bytearray_set256(ba: ByteArray, offset: uint, value: uint) -> ByteArray;
import throw func bytearray_marshalFull(ba: ByteArray) -> MarshalledBytes;
import throw func bytearray_extract(ba: ByteArray, offset: uint, nbytes: uint) -> ByteArray;
import throw func bytearray_copy(from: ByteArray, fromOffset: uint, to: ByteArray, toOffset: uint, nbytes: uint) -> ByteArray;

import throw func marshalledBytes_hash(mb: MarshalledBytes) -> bytes32;


func main() {
	let j = 0;
	while (j < 1) {
		j = j+1;
		let i = 0;
		let ba = bytearray_new(33);
		while (i < 1024*1024) {
			ba = bytearray_setByte(ba, i, 123);
			i = i+1;
		}
	}
}

